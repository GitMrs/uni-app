/* 公共按钮 */
.body{
	padding: 20rpx;
}
.user-set-btn {
	width: 100%;
	margin: 20rpx 0;
	background: #FFE933;
	border: 0;
	color: #333333;
}
.uni-input{
	padding: 10rpx;
	border-bottom: 1px solid #EEEEEE;
}
.user-set-btn-disable{
	background: #F4F4F4;
	border: 1px solid #CCCCCC;
	color: #909090;
	pointer-events: disable;
}
.logoBg {
	width: 100%;
}
.login-head {
	padding: 10rpx 0;
	color: #9E9E9E;
}
.other-login-header {
	position: relative;
}
.other-login-header:after,
.other-login-header:before {
	position: absolute;
	content: "";
	height: 1rpx;
	width: 100rpx;
	top: 50%;
	background: #9E9E9E;
}
.rule {
	color: #2CAEFC;
}
.input_wrap {
	position: relative;
}
.forget {
	position: absolute;
	bottom: 15rpx;
	right: 30rpx;
}
.prefixNum{
	position: absolute;
	top: 15rpx;
	color: #000000;
	left: 30rpx;
}
.CodeBtn{
	position: absolute;
	padding: 0 10rpx;
	background: #F4F4F4;
	color: #909090;
	bottom: 15rpx;
	right: 30rpx;
}
.other-login-header:after {
	left: 20%;
}
.other-login-header:before {
	right: 20%;
}
.closeBtn {
	position: fixed;
	top: 60rpx;
	left: 30rpx;
	font-size: 44rpx;
	font-weight: bold;
	color: #332F0A;
}

