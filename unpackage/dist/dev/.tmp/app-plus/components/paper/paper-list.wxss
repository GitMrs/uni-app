
.paper-list {
	padding: 20rpx;
	border-bottom: 1px solid #eee;
}
.paper-list>view image {
	height: 100rpx;
	width: 100rpx;
	-webkit-flex-shrink: none;
	    -ms-flex-negative: none;
	        flex-shrink: none;
	border-radius: 100%;
}
.paper-list>view:nth-of-type(2) {
	margin-left: 30rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
}
.paper-list .name {
	font-size: 30rpx;
}
.paper-list .time {
	color: #9E9E9E;
	font-size: 20rpx;
}
.paper-list .desc {
	color: #999;
}

