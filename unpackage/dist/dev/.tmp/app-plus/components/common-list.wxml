<uview class="commom-list u-f animated fadeIn" vue-id="1" bind:__l="__l" vue-slots="{{['default']}}"><view class="commom-list-l"><image src="{{item.userpic}}" mode="widthFix" lazy-load></image></view><view class="commom-list-r"><view class="u-f-aj u-f-jsb"><view class="u-f-aj"><view>{{item.username}}</view><view class="{{['tag-sex icon iconfont',item.sex===0?'icon-nan':'icon-nv']}}">{{item.sex===0?"男":"女"}}</view></view><view class="u-f-aj"><view class="icon iconfont icon-zengjia"></view>关注</view></view><view>{{''+item.title+''}}</view><view class="u-f-aj"><block wx:if="{{item.video}}"><block><image src="{{item.titlepic}}" mode="widthFix" lazy-load></image><view class="commom-list-play icon iconfont icon-bofang"></view><view class="commom-list-playinfo">{{''+item.video.looknum+" 次播放 "+item.video.long+''}}</view></block></block><block wx:if="{{item.share}}"><block><view class="commom-list-share u-f-aj"><image src="{{item.share.pic}}" mode="widthFix" lazy-load></image><view>{{''+item.share.title+''}}</view></view></block></block><block wx:if="{{item.img}}"><block><image src="{{item.titlepic}}" mode="widthFix" lazy-load></image></block></block></view><view class="u-f-aj u-f-jsb"><view>{{''+item.addree+''}}</view><view class="u-f-aj"><view class="u-f-aj"><view class="icon iconfont icon-fenxiang"></view>{{item.sharenum+''}}</view><view class="u-f-aj"><view class="icon iconfont icon-pinglun1"></view>{{item.commentnum+''}}</view><view class="u-f-aj"><view class="icon iconfont icon-dianzan1"></view>{{item.commentnum+''}}</view></view></view></view></uview>